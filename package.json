{
    "name": "vscode-breakpoints",
    "displayName": "vscode-breakpoints",
    "description": "An extension for managing breakpoints in vscode",
    "version": "0.0.1",
    "publisher": "roblourens",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-breakpoints.exportBreakpoints"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "vscode-breakpoints.exportBreakpoints",
                "title": "Export Breakpoints"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/es6-promisify": "^6.0.0",
        "es6-promisify": "^6.0.0"
    }
}
